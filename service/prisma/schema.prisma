generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String?
  googleId  String?  @unique
  entries   Entry[]
}

model Entry {
  id     Int      @id @default(autoincrement())
  date   String
  did    Json
  will   Json
  userId Int
  user   User     @relation(fields: [userId], references: [id])
}